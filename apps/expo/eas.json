{
    "cli": {
        "version": ">= 3.3.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "resourceClass": "m-medium"
            },
            "env": {
                "REACT_NATIVE_NODE_MODULES_DIR": "../../../node_modules"
            },
            "android": {
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": true,
                "resourceClass": "m-medium"
            },
            "env": {
                "REACT_NATIVE_NODE_MODULES_DIR": "../../../node_modules"
            }
        },
        "production": {
            "ios": {
                "resourceClass": "m-medium"
            },
            "env": {
                "REACT_NATIVE_NODE_MODULES_DIR": "../../../node_modules"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
